{
  "name": "bueffelsoft/cyclos-rest-api",
  "type": "library",
  "description": "PHP client for the Cyclos REST API",
  "keywords": [
    "cyclos",
    "php",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/bueffelSOFT/cyclos-rest-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakob Schumann",
      "homepage": "https://www.vrok.de"
    }
  ],
  "require": {
    "php": "^7.4.9|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.5.6|^7.4.3",
    "guzzlehttp/psr7": "^1.8.4|^2.1.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": { "Cyclos\\Api\\" : "src/" }
  }
}
