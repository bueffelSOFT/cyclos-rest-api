{
  "name": "bueffelsoft/cyclos-rest-api",
  "type": "library",
  "description": "PHP client for the Cyclos REST API",
  "keywords": [
    "cyclos",
    "php",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/bueffelSOFT/cyclos-rest-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakob Schumann",
      "homepage": "https://www.vrok.de"
    }
  ],
  "require": {
    "php": ">=7.4.9",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": { "Cyclos\\Api\\" : "src/" }
  }
}
